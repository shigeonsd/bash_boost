setup () 
{ 
    TARGET="Date";
    use -s Date
}
test_Date_success () 
{ 
    : オブジェクト生成;
    local today=$(date '+%Y/%m/%d');
    Date obj = "${today}";
    : テストを実装する。;
    [ "${obj}" = "${today}" ] || return $(failure);
    : テスト成功;
    return $(success)
}
test_Date_failure () 
{ 
    : テストを実装する。;
    export today=$(date '+%Y/%m/%d');
    ( Date obj = "${today}xx" ) > /dev/null;
    [ $? -ne 0 ] || return $(failure);
    return $(success)
}
