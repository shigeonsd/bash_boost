setup () 
{ 
    TARGET="Date";
    use -s Date
}
test_Date_success () 
{ 
    : オブジェクト生成;
    today=$(today);
    Date obj = "${today}";

    : テストを実装する。;
    [ "${obj}" = "${today}" ] || return $(failure);

    : テスト成功;
    return $(success);
}
test_Date_failure () 
{ 
    : テストを実装する。;
    export today=$(today);
    (Date obj = "${today}xx") > /dev/null;
    [ $? -ne 0 ] || return $(failure);

    return $(success);
}
test_Date.begin_of_month_success () 
{ 
    local arg="1971/02/15";
    local expected="1971/02/01";
    : オブジェクト生成;
    Date obj = "${arg}";
    : テストを実装する。;
    [ "$(obj.begin_of_month)" = "${expected}" ] || return $(failure);
    : テスト成功;
    return $(success)
}
