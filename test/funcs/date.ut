setup () 
{ 
    source ./../../funcs/date.bash;
    : __do_test をオーバーライドしてテスト関数を変更する。;
    function ___do_test () 
    { 
        local _bool=${1};
        local func=${2};
        shift 2;
        local args="$@";
        ${func} "$@";
        ret=$?;
        echo "${func} ${args} => ${ret}";
        is_${_bool} ${ret};
        return $?
    }
}
test__now___success () 
{ 
    : テスト実行;
    do_test_t now || return $(failure);
    return $(success)
}
test__now___error () 
{ 
    : テストスキップ;
    return $(skipped);
}
